package com.ibm.training.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class EmployeeProgress {

	@Id
	int employee_id;
	int project_part_progress;

	public int getEmployee_id() {
		return employee_id;
	}

	public void setEmployee_id(int employee_id) {
		this.employee_id = employee_id;
	}

	public int getProject_part_progress() {
		return project_part_progress;
	}

	public void setProject_part_progress(int project_part_progress) {
		this.project_part_progress = project_part_progress;
	}

	public EmployeeProgress() {

	}

	public EmployeeProgress(int employee_id, int project_part_progress) {
		this.employee_id = employee_id;
		this.project_part_progress = project_part_progress;
	}

}
