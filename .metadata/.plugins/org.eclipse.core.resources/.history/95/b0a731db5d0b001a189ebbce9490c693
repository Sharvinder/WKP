package com.ibm.training.model;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class EmployeeGrowth {

	@Id
	Integer tracker_id;
	Integer emp_id;
	Integer project_part_progress;
	Integer month;
	Integer year;

	public int getTracker_id() {
		return tracker_id;
	}

	public void setTracker_id(int tracker_id) {
		this.tracker_id = tracker_id;
	}

	public int getEmployee_id() {
		return emp_id;
	}

	public void setEmployee_id(int employee_id) {
		this.emp_id = employee_id;
	}

	public int getProject_part_progress() {
		return project_part_progress;
	}

	public void setProject_part_progress(int project_part_progress) {
		this.project_part_progress = project_part_progress;
	}

	public int getMonth() {
		return month;
	}

	public void setMonth(int month) {
		this.month = month;
	}

	public int getYear() {
		return year;
	}

	public void setYear(int year) {
		this.year = year;
	}
	
	public EmployeeGrowth() {
	}

	public EmployeeGrowth(int tracker_id, int emp_id, int project_part_progress, int month, int year) {
		this.tracker_id = tracker_id;
		this.emp_id = emp_id;
		this.project_part_progress = project_part_progress;
		this.month = month;
		this.year = year;
	}
}
